{
    "type": "postgres",
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "username": "${DB_USERNAME}",
    "password": "${DB_PASSWORD}",
    "database": "${DB_DATABASE}",
    "entities": ["src/entity/**/*.ts"],
    "migrations": ["src/migrations/**/*.ts"],
    "cli": {
      "migrationsDir": "src/migrations"
    },
    "synchronize": false
}