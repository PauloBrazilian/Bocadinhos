{
    "name": "msStore",
    "version": "0.0.1",
    "description": "Awesome project developed with TypeORM.",
    "type": "commonjs",
    "scripts": {
        "dev": "tsc && node ./dist/index.js",
        "start": "node ./dist/index.js",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create src/migrations/migration",
        "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/datasource.ts",
        "build": "tsc"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^16.18.106",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "prettier": "^3.3.3",
        "ts-node": "10.9.1",
        "tsx": "^4.19.0",
        "typescript": "^5.5.4"
    },
    "dependencies": {
        "class-validator": "^0.14.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-async-errors": "^3.1.1",
        "pg": "^8.12.0",
        "reflect-metadata": "^0.2.2",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.20",
        "zod": "^3.23.8"
    }
}